package n2exercise1;

public class Main {

	public static void main(String[] args) {
		 // Invocando método estático slow()
        Carro.slow();
        
        // Criando uma instância de Carro
        Carro meuCarro = new Carro("Corolla", 150);
        
        // Invocando método não estático acelerar()
        meuCarro.acelerar();
        
        // Invocando método não estático freio()
        meuCarro.freio();

	}

}
